<templateSet group="Laravel Routes">
  <template name="LrRouteSetName" value="-&gt;name('$Name$');" description="" toReformat="false" toShortenFQNames="true">
    <variable name="Name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="LrRouteReturnView" value="return view('$ViewName$');" description="" toReformat="false" toShortenFQNames="true">
    <variable name="ViewName" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>